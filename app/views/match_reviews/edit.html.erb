<div id="wide_left_content" class="sect">
  <h2><%=h @title%></h2>
  <%= error_messages_for :match_review%>
  <% form_for :match_review, :url=>match_review_path(@match_review), :html=>{:method=>:put} do |f| %>
    <%= render :partial=>'form', :locals => {:f=>f } %>
    <p>
      <%= f.submit "修改" %>
    </p>
  <% end %>
</div>

<div id="narrow_right_content" class="sect">
  <h2></h2>
  <ul class="sidebar_menu">
    <li>
      > <%= link_to '返回球评', match_review_path(@match_review) %>
    </li>
  </ul>
</div>
<div class="cb"></div>
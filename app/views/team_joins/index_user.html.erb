<div id="narrow_right_content" class="sect">
  <h2></h2>
  <%=render :partial=>'sidebar_menus/team_view_team_and_management'%>
</div>

<div id="wide_left_content">
  <%if @others.current_page == 1%>
    <div class="sect">
      <h2>管理员</h2>
      <%= user_icon(@admin)%>
      <div class="cb"></div>
    </div>

    <div class="sect">
      <h2>队员</h2>
      <%= user_icon(@players)%>
      <div class="cb"></div>
    </div>
  <%end%>

  <div class="sect">
    <h2>其他成员</h2>
    <%= user_icon(@others)%>
    <div class="cb"></div>
    <%= will_paginate @others %>
  </div>
</div>

<div class="cb"></div>
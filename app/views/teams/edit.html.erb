<%= error_messages_for :team %>
<% form_for @team do |f| %>

<%= render :partial => "form", :locals=>{:f=> f}%>

<p><%= submit_tag '修改' %></p>
<% end %>

<% form_for(@team, :html => { :multipart => true }) do |f| %>
  <%= team_image_tag @team %>
  <p>
    <label>更改队标</label><br/>
    <%= f.file_field  :uploaded_data %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

